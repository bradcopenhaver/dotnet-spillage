@{
    ViewData["Title"] = "Pile Spillage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<PileSpillage.Models.Question>

<h2>Pile Spillage</h2>
<hr />
@foreach(var question in Model)
{
